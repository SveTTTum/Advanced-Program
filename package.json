    "@qavajs/html-formatter": "^0.0.10",
    "cucumber-junit-reporter": "^0.0.1",
    "jest": "^29.5.0",
    "jest-playwright-preset": "^3.0.1",
    "playwright": "^1.32.3",
    "playwright-chromium": "^1.33.0",
    "winston": "^3.8.2",
    "xray-formatter": "^2.1.5"
